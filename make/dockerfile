#!/bin/bash

docker stop dp_gitlab_ci 2>/dev/null
docker rm dp_gitlab_ci 2>/dev/null
docker image build -t gitlab-ci -f Dockerfile --build-arg HOST_SSH_PRIVATE="$HOST_SSH_PRIVATE" --build-arg HOST_SSH_PUBLIC="$HOST_SSH_PUBLIC" --build-arg DEBUG="$DEBUG" $1
docker container run --name dp_gitlab_ci -d gitlab-ci
